{
    "manifest_version": 2,
    
    "name": "Chrome export tabs",
    "description": "Export the url of tabs opened at chrome.",
    "version": "1.0",
    
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Export tabs"
},
"permissions": [
    "tabs",
    "activeTab"
]}